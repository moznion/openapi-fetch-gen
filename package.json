{
  "name": "openapi-fetch-gen",
  "version": "0.0.1",
  "description": "Generate TypeScript API client from OpenAPI TypeScript definitions",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "biome check src",
    "fix": "biome check --write src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "bin": {
    "openapi-fetch-gen": "./dist/cli.js"
  },
  "keywords": [
    "openapi",
    "typescript",
    "api",
    "client",
    "generator",
    "openapi-typescript",
    "openapi-fetch"
  ],
  "author": "moznion",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "ts-morph": "^20.0.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^20.8.2",
    "@vitest/coverage-v8": "3.1.1",
    "openapi-fetch": "^0.8.1",
    "vitest": "3.1.1"
  },
  "peerDependencies": {
    "openapi-fetch": ">=0.7.0"
  }
}
