{
  "name": "@moznion/openapi-fetch-gen",
  "version": "0.0.1",
  "description": "Generate TypeScript API client from OpenAPI TypeScript interface definitions created by openapi-typescript",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "biome check src",
    "fix": "biome check --write src",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "bin": {
    "openapi-fetch-gen": "./dist/cli.js"
  },
  "keywords": [
    "openapi",
    "typescript",
    "api",
    "client",
    "generator",
    "openapi-typescript",
    "openapi-fetch"
  ],
  "author": "moznion",
  "license": "MIT",
  "dependencies": {
    "commander": "^13.1.0",
    "ts-morph": "^25.0.1",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.14.1",
    "@vitest/coverage-v8": "^3.1.1",
    "openapi-fetch": "^0.13.5",
    "vitest": "^3.1.1"
  },
  "peerDependencies": {
    "@biomejs/biome": ">=1.9.4",
    "openapi-fetch": ">=0.13.5"
  }
}
