{
  "name": "@moznion/openapi-fetch-gen",
  "version": "0.0.1",
  "description": "Generate TypeScript API client from OpenAPI TypeScript interface definitions created by openapi-typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "clean": "rm -rf ./dist/*",
    "build": "pnpm clean && tsc",
    "lint": "biome check src",
    "fix": "biome check --write src",
    "test": "vitest run",
    "test:watch": "vitest",
    "generate_test_resource": "openapi-typescript -o ./src/test_resources/schema.d.ts ./src/test_resources/schema.yaml"
  },
  "bin": {
    "openapi-fetch-gen": "./dist/cli.js"
  },
  "keywords": [
    "openapi",
    "typescript",
    "api",
    "client",
    "generator",
    "openapi-typescript",
    "openapi-fetch"
  ],
  "author": "moznion",
  "repository": {
    "type": "git",
    "url": "https://github.com/moznion/openapi-fetch-gen.git"
  },
  "bugs": {
    "url": "https://github.com/moznion/openapi-fetch-gen/issues"
  },
  "homepage": "https://github.com/moznion/openapi-fetch-gen#readme",
  "license": "MIT",
  "dependencies": {
    "commander": "^13.1.0",
    "ts-morph": "^25.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.14.1",
    "@types/tmp": "^0.2.6",
    "openapi-fetch": "^0.13.5",
    "openapi-typescript": "^7.6.1",
    "tmp": "^0.2.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1"
  },
  "peerDependencies": {
    "@biomejs/biome": ">=1.9.4",
    "openapi-fetch": ">=0.13.5"
  }
}
